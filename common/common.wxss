.wrap {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 999;
    background: rgba(0, 0, 0, 0.7);
    width: 100%;
    height: 100%;
}

.search {
    background: #fff;
    display: flex;
    height: 88rpx;
    align-items: center;
    padding: 0 30rpx;
    justify-content: space-between;
}

.search-block {
    height: 60rpx;
    line-height: 60rpx;
    background: #eee;
    border-radius: 8rpx;
    flex-grow: 1;
    text-align: center;
    margin-right: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.search-block image {
    width: 28rpx;
    height: 28rpx;
    margin-right: 16rpx;
}

.search-block text {
    font-size: 24rpx;
    color: #b2b2b2;
}

.search-question {
    background: url('data:image/png;base64,')no-repeat center left;
    background-size: 32rpx 36rpx;
    padding-left: 48rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #333;
}