/* 通用样式 */

page {
    font-family: "PingFang SC";
    background: #f3f5f7;
}

.content {
    width: 100%;
    height: 100%;
    overflow: scroll;
}

.border-1px {
    position: relative;
}

.border-1px:after {
    position: absolute;
    content: "";
    height: 1px;
    background: #dbdbdb;
    width: 100%;
    bottom: 0;
    left: 0;
    -webkit-transform: scaleY(.5);
    -ms-transform: scaleY(.5);
    -o-transform: scaleY(.5);
    transform: scaleY(.5);
}

.border-1px-e5 {
    position: relative;
}

.border-1px-e5:after {
    position: absolute;
    content: "";
    height: 1px;
    background: #dbdbdb;
    width: 100%;
    bottom: 0;
    left: 0;
    -webkit-transform: scaleY(.5);
    -ms-transform: scaleY(.5);
    -o-transform: scaleY(.5);
    transform: scaleY(.5);
}

