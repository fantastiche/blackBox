@import "../../common/common.wxss";
@import "../index/index.wxss";


.choose-block {
  display: flex;
  height: 80rpx;
  padding-left: 30rpx;
  background: #fff;
  align-items: center;
  margin-bottom: 30rpx;
}
.choose-block text {
  display: inline-block;
  padding: 10rpx 30rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
  border-radius: 8rpx;
}
.choose-active {
  background: #333 !important;
  color: #fff !important;
}