@import "../../common/common.wxss";
.mine-block {
  background: #fff;
}

.mine-info-wrap {
  margin-left: 30rpx;
  padding: 30rpx 30rpx 30rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.mine-info {
  display: flex;
}

.mine-info-head {
  width: 110rpx;
  height: 110rpx;
  position: relative;
}

.mine-info .head {
  width: 110rpx;
  height: 110rpx;
  position: absolute;
  z-index: 999;
}

.authentication {
  width: 24rpx;
  height: 28rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 1001;
}

.mine-info-detail {
  position: relative;
}

.mine-info-detail .level {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto 0 auto 16rpx;
}

.attention {
  font-size: 32rpx;
  color: #fff;
  padding: 0 40rpx;
  background: #333;
  height: 68rpx;
  line-height: 68rpx;
  border-radius: 8rpx;
}

.bottom-ctrl {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 100rpx;
  border-top: 1rpx solid #dbdbdb;
}

.bottom-ctrl view {
  background-size: 40rpx 40rpx;
  padding-top: 52rpx;
  line-height: 0;
}

.bottom-ctrl text {
  font-size: 24rpx;
  line-height: 1;
  color: #333;
}

.no-good {
  background: url('data:image/png;base64,') no-repeat center top;
}

.no-collecte {
  background: url('data:image/png;base64,') no-repeat center top;
  width: 90rpx;
  text-align: center;
}

.comment {
  background: url('data:image/png;base64,') no-repeat center top;
}

.good {
  background: url('data:image/png;base64,')no-repeat center top
}

.good text {
  color: #fe6270;
}

.collecte {
  background: url('data:image/png;base64,')no-repeat center top;
}
.collecte text {
  color: #fe6270;
}